#!/bin/zsh

# Update Homebrew 
brew update

# Update any installed formulae
brew upgrade

# Set default app by command
brew install duti

# Tap drivers
brew tap homebrew/cask-drivers

# Install dev tools
brew install alacritty
brew install rectangle
brew install tmux
brew install drawio
brew install dbeaver-community
brew install nvm

# Install drivers
brew install logitech-options

# PDF reader
brew adobe-acrobat-reader